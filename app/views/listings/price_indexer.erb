<h1>price change indexer</h1>

<table>
  <thead>
    <tr>
      <th>Old Price</th>
      <th>New Price</th>
      <th>Updated At</th>
    </tr>
  </thead>

  <tbody>
    <% @price_history.each do |price| %>
      <tr>
        <td><%= price.old_price %></td>
        <td><%= price.new_price %></td>
        <td><%= price.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

